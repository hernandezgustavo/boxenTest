server {
  access_log <%= scope.lookupvar "nginx::config::logdir" %>/resourcemanagement-api.access.log main;
  listen 80;
  server_name devrm.daptiv.com;
  ignore_invalid_headers off;

  location / {
    proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
    proxy_set_header Host $http_host;
    proxy_redirect off;
    proxy_pass http://localvm.daptiv.com:9089;
  }}
